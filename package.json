    "test": "jest",
    "tsc": "tsc -p .",
    "xcode": "open ios/$npm_package_name.xcodeproj"
  },
  "dependencies": {
    "react": "16.5.0",
    "react-native": "0.57.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.0",
    "@types/react-test-renderer": "^16.0.2",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "husky": "^1.0.1",
    "jest": "23.6.0",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "0.47.0",
    "prettier": "^1.14.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.5.0",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
